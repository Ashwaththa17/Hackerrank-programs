import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner scanner = new Scanner(System.in);
        long N = scanner.nextInt();
        long sum=0;
        long product=1;
        long temp=N;
        while(temp>0){
            long digit=temp%10;
            sum += digit;
            product *= digit;
            temp/=10;
        }
        if(sum == product){
            System.out.println("Spy Number");
        }else{
            System.out.println("Not a Spy Number");
        }
    }
}
