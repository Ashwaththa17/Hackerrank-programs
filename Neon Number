import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n=s.nextInt();
        int sum=0;
        
        int l=n*n;
        
        while(l!=0){
            int a=l%10;
            sum+=a;
            l=l/10;    
        }
        
        if(n == sum)
            System.out.println("true");
        else
            System.out.println("false");
            
    }
}
