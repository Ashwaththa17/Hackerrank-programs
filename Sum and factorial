import java.util.*;
public class Solution {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int N=scanner.nextInt();
        int sum=0;
        int factorial=1;
        for(int i=1;i<=N;i++){
            sum+=i;
            factorial*=i;
        }
        System.out.println(sum);
        System.out.println(factorial);
    }
}
