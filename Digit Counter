import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        int c=0;
        if(n==0)
            c++;
       
        
        while(n!=0){
            c++;
            n=n/10;
        }
        System.out.println(c);
    }
}
