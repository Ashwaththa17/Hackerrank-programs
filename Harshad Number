import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n=s.nextInt();
        int sum=0;
        int t=n;
        
        while(n!=0){
            int l=n%10;
            sum+=l;
            n=n/10;
        }
        
        if(t%sum == 0)
            System.out.println("Harshad Number");
        else
            System.out.println("Not Harshad Number");

    }
}
